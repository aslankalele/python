answer = input().lower()

good_responses = ["—Ö–æ—Ä–æ—à–æ", "–Ω–æ—Ä–º–∞–ª—å–Ω–æ", "–æ—Ç–ª–∏—á–Ω–æ"]
bad_responses = ["–ø–ª–æ—Ö–æ", "–Ω–µ —Ö–æ—Ä–æ—à–æ", "..."]

if answer in good_responses:
    print("üòä")
elif answer in bad_responses:
    print("üòä")  
else:
    print("üòä")
